{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["esnext"],
    "pretty": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["src/*"],
      "@bot": ["src/bot"],
      "@bot/*": ["src/bot/*"],
      "@commands/*": ["src/bot/commands/*"],
      "@commands": ["src/bot/commands"]
    }
  },
  "include": ["src/**.ts"],
  "exclude": ["**/node_modules", "**/dist"]
}
