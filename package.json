{
  "name": "lily-pad-assistant",
  "version": "1.0.0",
  "description": "Simple commands bot for the Lily Pad Telegram Chat.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "commands": "node bot/commands/index.js",
    "build": "tsc && tsc-alias",
    "dev": "tsx --watch src/index.ts",
    "start": "tsx dist/index.js",
    "start-pm2": "pm2 start dist/index.js --node-args=\"-r tsconfig-paths/register\"",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "telegraf": "^4.12.2"
  },
  "devDependencies": {
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  }
}
